#! /bin/sh

set -e

ICONDIR=/usr/share/icons
THEMES="breeze_cursors Breeze_Snow"

case "$1" in
    remove|deconfigure|failed-upgrade)
        for theme in $THEMES; do
            update-alternatives --remove x-cursor-theme \
                $ICONDIR/$theme/index.theme
        done
  ;;
esac

